<%- include("partials/header") %> 
<h1>Home</h1>
<p><%= homeStartingContent %> </p>
<% posts.forEach(post => { %>
    <h3><%= post.title %></h3>
    <% const myTruncatedString = post.content.substring(0,100) + "..."; %> 
    <p><%= myTruncatedString %> <a href="post/<%= post._id %>">Read more</a> </p>
<% });  %>
<%- include("partials/footer") %>